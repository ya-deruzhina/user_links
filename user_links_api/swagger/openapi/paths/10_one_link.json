{
    "/api/v1/links/{link_id}/": {
        "get": {
            "description": "Show one Link",
            "tags": [
                "Links"
            ],
            "parameters": [
                {
                  "name": "link_id",
                  "in": "path",
                  "description": "Id",
                  "schema": {
                    "type": "integer"
                  }
                }
              ],
            "responses": {
                "200": {
                    "description": "Page of one Link",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "description": "Page of one Link",
                                "properties": {
                                        "type": "array",
                                        "results": {"$ref": "#components/schemas/Link"}
                                    }
                                }
                            }
                        }
                    },

                "404": {
                    "description": "Returned after sending invalid id",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "errors": {
                                        "type": "object",
                                        "example": {
                                            "Error": [
                                                "not found"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }

            }
        },
        "patch": {
            "description": "Update one Link",
            "tags": [
                "Links"
            ],
            "parameters": [
                {
                  "name": "link_id",
                  "in": "path",
                  "description": "Id",
                  "schema": {
                    "type": "integer"
                  }
                }
              ],
            "requestBody": {
                "$ref": "#/components/requestBodies/LinkUpdateRequestBody"
            },
            "responses": {
                "200": {
                    "description": "Page of one Link",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "description": "Page of one Link",
                                "properties": {
                                        "type": "array",
                                        "results": {"$ref": "#components/schemas/Link"}
                                    }
                                }
                            }
                        }
                    },
                "404": {
                    "description": "Returned after sending invalid id",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "errors": {
                                        "type": "object",
                                        "example": {
                                            "Error": [
                                                "not found"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }

            }
        },
        "delete": {
            "description": "Delete one Link",
            "tags": [
                "Links"
            ],
            "parameters": [
                {
                  "name": "link_id",
                  "in": "path",
                  "description": "Id",
                  "schema": {
                    "type": "integer"
                  }
                }
              ],
            "responses": {
                "200": {
                    "description": "Returned after sending valid id",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "errors": {
                                        "type": "object",
                                        "example": {
                                            "Status": [
                                                "Deleted"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                },

                "404": {
                    "description": "Returned after sending invalid id",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "errors": {
                                        "type": "object",
                                        "example": {
                                            "Error": [
                                                "not found"
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }

            }
        }
    }
}