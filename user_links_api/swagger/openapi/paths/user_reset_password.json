                        "application/json": {
                            "schema": {
                                "properties": {
                                    "Status": {
                                        "type":"string",
                                        "example":"Unsuccessful: Invalid Data"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
